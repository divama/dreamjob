/*******************************
         Theme Overrides
*******************************/
.dj .ui.dividing.header:first-child {    
    margin-top: 1em;
}

.ui.header + .dj-quote p {
        font-family: @fontName, Arial, Helvetica, sans-serif;
        font-size:1.2em;
} 

.dj-themeColor {
    h1:first-child, 
    h2:first-child, 
    h3:first-child, 
    h4:first-child, 
    h5:first-child{
        margin-top: 0.78em;
    }
}